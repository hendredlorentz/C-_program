</span><span style="color:#008080;">26</span> <span style="color:#000000;">    signal(SIGQUIT,fun_d);
</span><span style="color:#008080;">27</span>     <span style="color:#0000ff;">if</span>(sigemptyset(&amp;<span style="color:#0000ff;">set</span>)&lt;<span style="color:#800080;">0</span><span style="color:#000000;">)
</span><span style="color:#008080;">28</span>         perror(<span style="color:#800000;">&#34;</span><span style="color:#800000;">init sign error!</span><span style="color:#800000;">&#34;</span><span style="color:#000000;">);
</span><span style="color:#008080;">29</span>     <span style="color:#0000ff;">if</span>(sigaddset(&amp;<span style="color:#0000ff;">set</span>,SIGTSTP)&lt;<span style="color:#800080;">0</span><span style="color:#000000;">)
</span><span style="color:#008080;">30</span>         perror(<span style="color:#800000;">&#34;</span><span style="color:#800000;">add ctrl&#43;z error!\n</span><span style="color:#800000;">&#34;</span><span style="color:#000000;">);
</span><span style="color:#008080;">31</span>     <span style="color:#0000ff;">if</span>(sigaddset(&amp;<span style="color:#0000ff;">set</span>,SIGQUIT)&lt;<span style="color:#800080;">0</span><span style="color:#000000;">)
</span><span style="color:#008080;">32</span>             perror(<span style="color:#800000;">&#34;</span><span style="color:#800000;">ass &#39;ctrl&#43;\&#39; error!\n</span><span style="color:#800000;">&#34;</span><span style="color:#000000;">);
</span><span style="color:#008080;">33</span>     <span style="color:#0000ff;">while</span>(<span style="color:#800080;">1</span><span style="color:#000000;">)
