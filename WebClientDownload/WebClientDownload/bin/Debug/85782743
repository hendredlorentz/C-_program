</span><span style="color:#008080;"> 8</span>     <span style="color:#0000ff;">int</span><span style="color:#000000;"> i;
</span><span style="color:#008080;"> 9</span>     <span style="color:#0000ff;">for</span>(i&#61;<span style="color:#800080;">0</span>;i&lt;&#61;<span style="color:#800080;">7</span>;i&#43;&#43;<span style="color:#000000;">)
</span><span style="color:#008080;">10</span> <span style="color:#000000;">    {
</span><span style="color:#008080;">11</span>     printf(<span style="color:#800000;">&#34;</span><span style="color:#800000;">running…\n</span><span style="color:#800000;">&#34;</span><span style="color:#000000;">);
</span><span style="color:#008080;">12</span>     sleep(<span style="color:#800080;">1</span><span style="color:#000000;">);
</span><span style="color:#008080;">13</span> <span style="color:#000000;">    }
