</span><span style="color:#008080;">35</span>         printf(<span style="color:#800000;">&#34;</span><span style="color:#800000;">Ctrl &#43;z and &#39;Ctrl &#43;\&#39; is zuse!\n</span><span style="color:#800000;">&#34;</span><span style="color:#000000;">);
</span><span style="color:#008080;">36</span>         sleep(<span style="color:#800080;">2</span><span style="color:#000000;">);
