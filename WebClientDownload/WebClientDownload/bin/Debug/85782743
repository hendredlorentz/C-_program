</span><span style="color:#008080;">28</span>         <span style="color:#0000ff;">if</span>((n &#61;read(pipe_fd[<span style="color:#800080;">0</span>],buf1,<span style="color:#800080;">100</span>))&gt;<span style="color:#800080;">0</span><span style="color:#000000;">)
